<VirtualHost *:80>
    DocumentRoot %TRAVIS_BUILD_DIR%/public
    ServerName localhost

    <Directory "%TRAVIS_BUILD_DIR%/public">
        Options -Indexes +FollowSymLinks +MultiViews
        AllowOverride All
        Require all granted
    </Directory>
    
     ProxyPassMatch ^/(.*\.php)$ fcgi://php:9000%TRAVIS_BUILD_DIR%/public/$1
</VirtualHost>
